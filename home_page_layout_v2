		<script>
			// Initialize a variable to keep track of the current slide index.
			let slideIndex = 1;

			// Call the function 'showSlides' to display the first slide when the page loads.
			showSlides(slideIndex);

			// Function to display the slides based on the provided slide index 'n'.
			function showSlides(n) {
				let i;
				// Get all elements with the class name "mySlides" and store them in the 'slides' variable.
				let slides = document.getElementsByClassName("mySlides");


				// Hide all slides by setting their 'display' style property to "none".
				for (i = 0; i < slides.length; i++) {
					slides[i].style.display = "none";
				}


				// Display the slide with the current slide index by setting its 'display' style property to "block".
				slides[slideIndex - 1].style.display = "block";

			}
		</script>

		<script>
			//When you scrol on the window it calls a function
			window.onscroll = function(){progressbar()};
			function progressbar(){
				var x = document.body.scrollTop || document.documentElement.scrollTop; //what this variable is doing, like scrolling from top to bottom, height across
				var h = document.documentElement.scrollHeight - document.documentElement.clientHeight; //height vertical
				var s =(x/h) * 100; //calculating the percentage of both heights, if top is 10% then vertical is 10% as well
				document.getElementById("pb").style.width = s + "%"; //links/attacth to the html at the top through the id "pb"
			}
		</script>


		<script>
			document.addEventListener('DOMContentLoaded', () => {
				let currentIndex = 0;
				const slides = document.querySelectorAll('.slide12');
				const dots = document.querySelectorAll('.dot11');
				const totalSlides = slides.length;

				const showSlide = (index) => {
					if (index >= totalSlides) {
						currentIndex = 0;
						index = 0;
					} else if (index < 0) {
						currentIndex = totalSlides - 1;
						index = totalSlides - 1;
					}

					const offset = -index * 8.2; 
					document.querySelector('.slider11').style.transform = `translateX(${offset}%)`;

					dots.forEach((dot, i) => {
						dot.classList.remove('activeclass');
						if (i === index) {
							dot.classList.add('activeclass');
						}
					});
				};

				const nextSlide = () => {
					currentIndex = (currentIndex + 1) ;
					showSlide(currentIndex);
				};

				const prevSlide = () => {
					currentIndex = (currentIndex - 1 + totalSlides);
					showSlide(currentIndex);
				};

				dots.forEach((dot, index) => {
					dot.addEventListener('click', () => {
						currentIndex = index;
						showSlide(currentIndex);
					});
				});

				document.querySelector('.next').addEventListener('click', nextSlide);
				document.querySelector('.prev').addEventListener('click', prevSlide);

				showSlide(currentIndex);
			});
		</script>
